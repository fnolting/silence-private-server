{
    "POLL": {
        "command": "$POLL\r\n",
        "maxretry": 3,
        "timeout": 60
    },
    "TURN_ON_SCOOTER": {
        "command": "$PWON,4\r\n",
        "maxretry": 3,
        "timeout": 60
    },
    "TURN_OFF_SCOOTER": {
        "command": "$PWON,0\r\n",
        "maxretry": 3,
        "timeout": 60
    },
    "FLASH": {
        "command": "$FIND,0,19,1,30,1\r\n",
        "maxretry": 1,
        "timeout": 300
    },
    "BEEP_FLASH": {
        "command": "$FIND,0,35,1,30,0\r\n",
        "maxretry": 1,
        "timeout": 30
    },
    "OPEN_SEAT": {
        "command": "$TCOP\r\n",
        "maxretry": 3,
        "timeout": 180
    },
    "STOP_FLASH": {
        "command": "$PWON,0\r\n",
        "maxretry": 3,
        "timeout": 300
    }
}
